{
  "name": "wxt-react-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/ensure-chrome-flags.mjs && wxt dev",
    "dev:firefox": "wxt -b firefox",
